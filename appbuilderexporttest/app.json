  {
      "expo": {
        "name": "AmazingDailyTrivia",
        "slug": "AmazingDailyTrivia",
        "platforms": [
          "ios",
          "android",
          "web"
        ],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/ficon.png",
        "splash": {
          "image": "./assets/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        },
        "updates": {
          "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
          "**/*"
        ],
        "ios": {
          "supportsTablet": true,
          "bundleIdentifier": "com.amazingdailytrivia.amazingdailytrivia"
        },
        "android": {
          "package": "com.amazingdailytrivia.amazingdailytrivia"
        },
        "entryPoint": "node_modules/expo/AppEntry.js"
      }
    }